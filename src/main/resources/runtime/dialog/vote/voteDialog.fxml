<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity"
      prefWidth="478.0" xmlns="http://javafx.com/javafx/8.0.171">
    <stylesheets>
        <URL value="@vote.css"/>
    </stylesheets>

    <Pane fx:id="header" prefHeight="52.0" prefWidth="478.0">
        <Button id="close" disable="true" layoutX="437.0" layoutY="13.0" mnemonicParsing="false"
                style="-fx-background-color: transparent;">
            <graphic>
                <Pane styleClass="snapes">
                    <Line endX="15.0" endY="15.0" strokeLineCap="ROUND" strokeWidth="4.0" styleClass="snape"/>
                    <Line endY="15.0" startX="15.0" strokeLineCap="ROUND" strokeWidth="4.0" styleClass="snape"/>
                </Pane>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Label layoutX="81.0" layoutY="-1.0" styleClass="VoteLable" text="Голосуй - Получай бонусы">
            <padding>
                <Insets top="20.0"/>
            </padding>
        </Label>
    </Pane>
    <VBox id="content" alignment="CENTER" layoutX="-4.0" layoutY="59.0" prefWidth="485.0">
        <VBox id="tops_root" alignment="CENTER" spacing="5.0"/>
        <Pane prefHeight="117.0" prefWidth="487.0">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <Label layoutX="65.0" layoutY="14.0" styleClass="VoteDescription"
                   text="Проголосуй везде и получи Донат-Кейс:"/>
            <ImageView id="case_image" fitHeight="80.0" fitWidth="80.0" layoutX="192.0" layoutY="54.0"
                       pickOnBounds="true" preserveRatio="true"/>
        </Pane>
    </VBox>
</Pane>
